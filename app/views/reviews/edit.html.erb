<h1>Edit your review</h1>

<%= form_with(model: [@product, @review]) do |form| %>
    <div>
        <%= form.label :comment%>
        <%= form.text_field :comment, required:true%>
    </div>
    <div>
        <%= form.label :rating%>
        <%= form.select :rating, (1..5), required:true%>
    </div>
    
    <%=form.submit "Edit review" %>
<% end %>
